{
  "manifest_version": 3,
  "name": "HTML Parser Extension",
  "version": "1.0",
  "description": "A Chrome extension to parse HTML and perform simple tasks",

  "action": {
    "default_icon": {
      "16": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    }
  },

  "background": {
    "service_worker": "background.js" 
  },

  "permissions": [
    "activeTab", 
    "storage"
  ],

  "icons": {
    "16": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },



  "content_scripts": [
    {
      "matches": ["https://cab.brown.edu/*"],
      "js": ["content.js"]
    }
  ]

}
